# <%= generated_by_babushka %>

server {
  listen 80;
  include vhosts/<%= var :domain %>.common;
}

<% if var(:enable_ssl, :default => 'no').starts_with?('y') %>
server {
  listen 443;

  ssl                  on;
  ssl_certificate      certs/<%= var :domain %>.crt;
  ssl_certificate_key  certs/<%= var :domain %>.key;
  ssl_session_timeout  5m;

  include vhosts/<%= var :domain %>.common;
}
<% end %>
